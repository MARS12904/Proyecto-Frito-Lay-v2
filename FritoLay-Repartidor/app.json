{
  "expo": {
    "name": "Frito Lay Repartidor",
    "slug": "fritolay-repartidor",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "fritolay-repartidor",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Esta app necesita acceso a tu ubicación para rastrear las entregas.",
        "NSCameraUsageDescription": "Esta app necesita acceso a la cámara para tomar fotos de las entregas."
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/icon.png"
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "CAMERA"
      ],
      "edgeToEdgeEnabled": true
    },
    "web": {
      "output": "static",
      "favicon": "./assets/icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-secure-store",
      [
        "expo-image-picker",
        {
          "photosPermission": "La app necesita acceso a tus fotos para que puedas seleccionar fotos de entrega."
        }
      ],
      "expo-camera",
      "expo-location"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "supabaseUrl": "https://abledixphpivpvcajhdt.supabase.co",
      "supabaseAnonKey": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpc3MiOiJzdXBhYmFzZSIsInJlZiI6ImFibGVkaXhwaHBpdnB2Y2FqaGR0Iiwicm9sZSI6ImFub24iLCJpYXQiOjE3NjMzMTM4NDcsImV4cCI6MjA3ODg4OTg0N30.w26vd6tMo0lMogtjeRgMOTk2bX_yLde0Lr3-qVGigUw"
    }
  }
}
